{
  "name": "spotifood-wrapper",
  "version": "1.0.3",
  "description": "The wrapper for web api of spotify",
  "main": "lib/index.js",
  "scripts": {
    "clear": "rimraf lib",
    "clear:coverage": "rimraf coverage",
    "clear:nyc": "rimraf .nyc_output",
    "build": "npm run clear && ./node_modules/.bin/babel --out-dir lib src",
    "build:watch": "npm run build -- --watch",
    "build:umd": "./node_modules/.bin/webpack --output-filename spotify-wrapper.umd.js",
    "build:umd:watch": "./node_modules/.bin/webpack --output-filename spotify-wrapper.umd.js --watch",
    "build:umd:min": "./node_modules/.bin/webpack --output-filename spotify-wrapper.umd.min.js -p",
    "build:all": "npm run build && npm run build:umd && npm run build:umd:min",
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint",
    "test": "./node_modules/.bin/mocha test/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha test/**/*.spec.js --require babel-register --watch",
    "test:coverage": " npm run clear:nyc && npm run clear:coverage && nyc npm test",
    "coveralls": "npm run test:coverage && nyc report --reporter=text-lcov | coveralls",
    "prepublish": "npm run build:all"
  },
  "files": [
    "dist",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mateussmohamed/spotifood-wrapper.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library",
    "spotify"
  ],
  "author": "Mateus Santana <mateuss.dev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mateussmohamed/spotifood-wrapper/issues"
  },
  "homepage": "https://github.com/mateussmohamed/spotifood-wrapper#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.3.2",
    "babel-register": "^6.24.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "^0.14.3",
    "mocha": "^4.0.1",
    "node-fetch": "^1.7.3",
    "nyc": "^11.3.0",
    "prettier-eslint": "^8.2.2",
    "rimraf": "^2.6.1",
    "sinon": "^4.1.2",
    "sinon-chai": "^2.14.0",
    "sinon-stub-promise": "^4.0.0",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "js-cookie": "^2.2.0"
  }
}
